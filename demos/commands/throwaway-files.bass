(def (repo) *args*)

(run ($ .go "test" "-coverprofile" ./cover.out
        :from repo))

(run ($ .go "tool" "cover" "-html" ./cover.out "-o" ./cover.html
   		:from repo))

(run ($ .luakit ./cover.html))

; or...
(run-seq
  ($ .go "test" "-coverprofile" ./cover.out :from repo)
  ($ .go "tool" "cover" "-html" ./cover.out "-o" ./cover.html :from repo)
  ($ .luakit ./cover.html))

; no paths are emitted, so all files created in the working directory are GCed
