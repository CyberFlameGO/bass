(def create-dir
  "mkdir some-dir; echo 42 > some-dir/some-file")

(def created
  (let [create (-> ($ .sh -c $create-dir)
                   alpine-test)]
    (run create)
    (path create ./)))

(next (run (-> ($ .cat ./foo/some-file)
               (with-mount created/some-dir/ ./foo/)
               alpine-test)))
